{
  command_timeout = 2000;
<% toml.each do |key, value| -%>
<% if value.class == String -%>
  <%= key -%> = "<%= value -%>";

<% else -%>
<% next if key == 'directory' -%>
  <%= key -%> = {
<% value.each do |k,v| -%>
    <%= k.strip -%> = <%= v.class == String ? "\"#{v.strip}\"" : v -%>;
<% end -%>
  };
<% end -%>
<% end -%>
}
